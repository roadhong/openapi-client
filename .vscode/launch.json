{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "OpenAPI Client Run",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["run", "dev"],
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "VITE_CUSTOM_SERVER_URL": "http://localhost:20000",
        "VITE_OPENAPI_URL": "http://localhost:20000/api-metadata.json"
      },
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
